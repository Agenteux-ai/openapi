{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "properties" : {
    "account" : {
      "$ref" : "../components/schemas/AccountReference.json",
      "readOnly" : true
    },
    "createdAt" : {
      "$ref" : "../components/schemas/CreatedAt.json"
    },
    "description" : {
      "type" : "string",
      "maxLength" : 1024
    },
    "extended" : {
      "type" : "object",
      "properties" : {
        "map" : {
          "type" : "object"
        }
      }
    },
    "id" : {
      "type" : "string",
      "format" : "uuid",
      "readOnly" : true
    },
    "name" : {
      "type" : "string",
      "maxLength" : 256
    },
    "schema" : {
      "$ref" : "../components/schemas/UrlReference.json"
    },
    "slug" : {
      "type" : "string",
      "maxLength" : 256,
      "pattern" : "[a-z_\\-0-9]+"
    },
    "updatedAt" : {
      "$ref" : "../components/schemas/UpdatedAt.json"
    }
  },
  "required" : [ "name" ],
  "$id" : "https://schema.agenteux.com/toggle/Stage.json",
  "title" : "Stage",
  "description" : "JSON Schema definition for Stage"
}